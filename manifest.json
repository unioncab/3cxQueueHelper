{
  "name": "3cxClipChrome",
  "description": "Clip phone number from 3CX web client",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "src/options/options.html",
    "default_icon": "src/images/clip-line-icon.png"
  },
  "background": {
    "service_worker": "src/scripts/background.js"
  },
  "permissions": ["storage"]
}
